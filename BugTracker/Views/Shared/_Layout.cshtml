<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BugTracker</title>


    <link rel="stylesheet" href="~/BugTracker.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/bootstrap-slate.css" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="~/css/datatables.css">
    <link rel="stylesheet" href="@(nameof(BugTracker)).styles.css" />



</head>
<body>
    <partial name="_NavBar"></partial>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - BugTracker -
            <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a> -
            <a href="https://lhermann.netlify.app/">Meet the Creator</a>
        </div>
    </footer>

    <script type="text/javascript" src="~/lib/jquery/jquery.min.js"></script>
    <script type="text/javascript" charset="utf8" src="~/js/datatables.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/popper.js/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/cbc67c8581.js" crossorigin="anonymous"></script>
    <script src="~/lib/toastify-js/toastify.min.js"></script>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
